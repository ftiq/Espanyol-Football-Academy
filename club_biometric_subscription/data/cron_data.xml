<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cron_update_subscriptions" model="ir.cron">
        <field name="name">Update Subscription Status</field>
        <field name="model_id" ref="model_club_subscription"/>
        <field name="state">code</field>
        <field name="code">model.cron_check_subscriptions()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">True</field>
    </record>
    <record id="cron_sync_biometric" model="ir.cron">
        <field name="name">Sync Biometric Status</field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="state">code</field>
        <field name="code">model.update_subscription_status()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="active">True</field>
    </record>
</odoo>
